<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="login_pane" prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/13" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LoginController">
   <children>
      <Label layoutX="95.0" layoutY="54.0" text="Login">
         <font>
            <Font name="Century" size="16.0" />
         </font>
      </Label>
      <Button layoutX="81.0" layoutY="165.0" mnemonicParsing="false" onMouseClicked="#onLoginClicked" prefHeight="40.0" prefWidth="71.0" text="Login">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </Button>
      <TextField fx:id="txt_username" layoutX="42.0" layoutY="86.0" promptText="Enter Username" />
      <TextField fx:id="txt_password" layoutX="42.0" layoutY="125.0" promptText="Enter Password" />
      <Label fx:id="lbl_error" layoutX="215.0" layoutY="205.0" prefHeight="16.0" prefWidth="170.0" />
      <Label layoutX="50.0" layoutY="227.0" text="New User?">
         <font>
            <Font name="Century" size="10.0" />
         </font>
      </Label>
      <Label layoutX="196.0" layoutY="22.0" text="Project Manager">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="28.0" />
         </font>
      </Label>
      <Button layoutX="103.0" layoutY="222.0" mnemonicParsing="false" onMouseClicked="#onSignupClicked" prefHeight="25.0" prefWidth="71.0" text="Sign Up">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </Button>
   </children>
   <stylesheets>
      <URL value="@styles.css" />
   </stylesheets>
</AnchorPane>
